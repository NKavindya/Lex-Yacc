// nested while loops
func nestedLoops(n : integer) -> integer {
    local i : integer;
    local j : integer;
    local sum : integer;
    sum := 0;
    i := 0;
    while (i < n) {
        j := 0;
        while (j < n) {
            sum := sum + i * j;
            j := j + 1;
        };
        i := i + 1;
    };
    return(sum);
}


