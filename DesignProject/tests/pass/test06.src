// Test 6: Complex expressions with logical operators
func evaluate(x : integer, y : integer, z : integer) -> integer {
    local result : integer;
    
    if (x > 0) then {
        if (y < 10) then {
            result := x + y;
        } else {
            result := z;
        };
    } else {
        result := z;
    };
    
    return(result);
}

